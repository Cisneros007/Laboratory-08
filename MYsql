CREATE TABLE `Eval02`.`Producto` ( `IdProducto` INT NOT NULL AUTO_INCREMENT COMMENT 'ID del producto' , `Nombre` VARCHAR(80) NOT NULL COMMENT 'Nombre' , `Descripcion` VARCHAR(250) NOT NULL COMMENT 'Descripci√≥n del producto' , `Stock` INT NOT NULL COMMENT 'Stock del producto' , `PrecioVenta` DECIMAL(40) NOT NULL COMMENT 'Precio de venta del producto' , PRIMARY KEY (`IdProducto`)) ENGINE = InnoDB;